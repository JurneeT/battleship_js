// First commit
class CPU 
{
  constructor(name)
  {
    this.name = name;
  }

  generate_random_attack(user_input)
  {
    return Math.floor(Math.random() * Math.floor(user_input))
  }
 

}


const cpu1 = new CPU('Mark');

